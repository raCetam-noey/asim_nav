<mvc:View
	controllerName="apppad.ext.main.MainPage"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:l="sap.ui.layout"
    xmlns:core="sap.ui.core"
    xmlns="sap.m"
	displayBlock="true"
	height="100%"
	busyIndicatorDelay="0"
>
<Page id="MainPage" showHeader="false">
 <landmarkInfo>
			<PageAccessibleLandmarkInfo id="_IDGenPageAccessibleLandmarkInfo1"
				rootRole="Region"
				rootLabel="{i18n>Home_rootLabel}"
				contentRole="Main"
				contentLabel="{i18n>Home_contentLabel}"/>
		</landmarkInfo>
		<l:BlockLayout
			id="BlockLayout"
			background="Default">
			<l:BlockLayoutRow id="_IDGenBlockLayoutRow1">
				<l:BlockLayoutCell id="_IDGenBlockLayoutCell1" class="sapUiNoContentPadding">
					<Image id="_IDGenImage1"
						src='./images/banner_ori (2).jpg'
						width="100%"
						height="100%"/>
				</l:BlockLayoutCell>
			</l:BlockLayoutRow>
			<l:BlockLayoutRow id="_IDGenBlockLayoutRow2">
				<l:BlockLayoutCell id="_IDGenBlockLayoutCell2"
					backgroundColorSet="ColorSet11"
					backgroundColorShade="ShadeD">
					<l:VerticalLayout id="_IDGenVerticalLayout1">
						<core:Icon id="_IDGenIcon1"
							src="sap-icon://role"
							size="2.5rem"
							color="Default"/>
						<Title
							id="administrationTile"
							text="{i18n>administrationTitle}"
							level="H3"
							titleStyle="H2"
							class="sapUiTinyMarginTopBottom"/>
						<Text id="_IDGenText1"
							text="{i18n>administrationContent}"
							class="sapUiTinyMarginBottom"/>
						<Link id="_IDGenLink1"
							text="{i18n>accessLink}"
							tooltip="{i18n>accessAdministrationLink}"/>
						<Link id="_IDGenLink2"
							text="{i18n>learnLink}"
							tooltip="{i18n>learnAdministrationTooltip}"/>
					</l:VerticalLayout>
				</l:BlockLayoutCell>
				<l:BlockLayoutCell id="_IDGenBlockLayoutCell3"
					backgroundColorSet="ColorSet11"
					backgroundColorShade="ShadeB">
					<l:VerticalLayout id="_IDGenVerticalLayout2">
						<core:Icon id="_IDGenIcon2"
							src="sap-icon://add-product"
							size="2.5rem"
							color="Default"/>
						<Title
							id="productTile"
							text="{i18n>productTitle}"
							level="H3"
							titleStyle="H2"
							class="sapUiTinyMarginTopBottom"/>
						<Text id="_IDGenText2"
							text="{i18n>productContent}"
							class="sapUiTinyMarginBottom"/>
						<Link id="_IDGenLink3"
							text="{i18n>accessLink}"
							tooltip="{i18n>accessProductsLink}"/>
						<Link id="_IDGenLink4"
							text="{i18n>learnLink}"
							tooltip="{i18n>learnProductsTooltip}"/>
					</l:VerticalLayout>
				</l:BlockLayoutCell>
				<l:BlockLayoutCell id="_IDGenBlockLayoutCell4"
					backgroundColorSet="ColorSet11"
					backgroundColorShade="ShadeD">
					<l:VerticalLayout id="_IDGenVerticalLayout3">
						<core:Icon id="_IDGenIcon3"
							src="sap-icon://customer-financial-fact-sheet"
							size="2.5rem"
							color="Default"/>
						<Title id="_IDGenTitle1"
							text="{i18n>revenueTitle}"
							level="H3"
							titleStyle="H2"
							class="sapUiTinyMarginTopBottom"/>
						<Text id="_IDGenText3"
							text="{i18n>revenueContent}"
							class="sapUiTinyMarginBottom"/>
						<Link id="_IDGenLink5"
							text="{i18n>accessLink}"
							tooltip="{i18n>accessRevenueLink}"/>
						<Link id="_IDGenLink6"
							text="{i18n>learnLink}"
							tooltip="{i18n>learnRevenueTooltip}"/>
					</l:VerticalLayout>
				</l:BlockLayoutCell>
				<l:BlockLayoutCell id="_IDGenBlockLayoutCell5"
					backgroundColorSet="ColorSet11"
					backgroundColorShade="ShadeB">
					<l:VerticalLayout id="_IDGenVerticalLayout4">
						<core:Icon
							id="supplierIcon"
							src="sap-icon://supplier"
							size="2.5rem"
							color="Default"
							/>
						<Title id="_IDGenTitle2"
							text="{i18n>supplierTitle}"
							level="H3"
							titleStyle="H2"
							class="sapUiTinyMarginTopBottom"/>
						<Text id="_IDGenText4"
							text="{i18n>supplierContent}"
							class="sapUiTinyMarginBottom"/>
						<Link id="_IDGenLink7"
							text="{i18n>accessLink}"
							tooltip="{i18n>accessSuppliersLink}"/>
						<Link id="_IDGenLink8"
							text="{i18n>learnLink}"
							tooltip="{i18n>learnSuppliersTooltip}"/>
					</l:VerticalLayout>
				</l:BlockLayoutCell>
			</l:BlockLayoutRow>
			<l:BlockLayoutRow id="_IDGenBlockLayoutRow3">
				<l:BlockLayoutCell id="_IDGenBlockLayoutCell6"
					backgroundColorSet="ColorSet11"
					backgroundColorShade="ShadeB">
					<l:VerticalLayout id="_IDGenVerticalLayout5">
						<core:Icon id="_IDGenIcon4"
							src="sap-icon://thumb-up"
							size="2.5rem"
							color="Default"/>
						<Title id="_IDGenTitle3"
							text="{i18n>socialMediaTitle}"
							level="H3"
							titleStyle="H2"
							class="sapUiTinyMarginTopBottom"/>
						<Text id="_IDGenText5"
							text="{i18n>socialMediaContent}"
							class="sapUiTinyMarginBottom"/>
						<Link id="_IDGenLink9"
							text="{i18n>accessLink}"
							tooltip="{i18n>accessSocialLink}"/>
						<Link id="_IDGenLink10"
							text="{i18n>learnLink}"
							tooltip="{i18n>learnSocialMediaTooltip}"/>
					</l:VerticalLayout>
				</l:BlockLayoutCell>
				<l:BlockLayoutCell id="_IDGenBlockLayoutCell7"					backgroundColorShade="ShadeA"
					width="3">
					
				</l:BlockLayoutCell>
			</l:BlockLayoutRow>
		</l:BlockLayout>
</Page>
</mvc:View>